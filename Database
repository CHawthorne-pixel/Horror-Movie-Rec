CREATE TABLE FILMS
  (FILM_ID NUMBER(9,0) NOT NULL,
  TITLE VARCHAR2(30),
  SUMMARY VARCHAR2(200),
  COUNTRY VARCHAR2(30),
  PRIMARY LANGUAGE VARCHAR2(30),
  RUNTIME NUMBER(3,0),
  
  PRIMARY KEY(FILM_ID)



